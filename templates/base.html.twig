<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block head_title %}
				Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link crossorigin="anonymous" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" rel="stylesheet"/>
			<style>
				body {
					padding: 15px;
				}
				table {
					margin-bottom: 16px;
				}
				th,
				td {
					padding: 0 10px;
					border: 1px solid;
				}
			</style>
		{% endblock %}
	</head>
	<body>
		{% block body %}
			<div class="container">
				<div class="row">
					<div class="col-3">
						{% include '@MazariniTest/step/menu.html.twig' with {'items': steps} %}
					</div>
					<div class="col-9">
						<h1>
							{% block title %}{% endblock %}
						</h1>
						{% block content %}{% endblock %}
					</div>
				</div>
			</div>
		{% endblock %}
		{% block javascripts %}{% endblock %}
	</body>
</html></body></html>
